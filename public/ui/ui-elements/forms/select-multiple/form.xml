


<form id="form" xmlns="https://hyperview.org/hyperview">
  


  
    <text style="description">Simple Select</text>
  

<select-multiple name="simple-select-multiple" style="select">
  <view style="select-separator"/>
  <option style="select-option" value="paid_parking">
    <text style="select-label">Paid parking</text>
    <view style="select-radio-outer">
      <view style="select-radio-inner"/>
    </view>
  </option>
  <view style="select-separator"/>
  <option style="select-option" value="free_parking">
    <text style="select-label">Free parking</text>
    <view style="select-radio-outer">
      <view style="select-radio-inner"/>
    </view>
  </option>
  <view style="select-separator"/>
  <option style="select-option" value="unknown_parking">
    <text style="select-label">Unknown</text>
    <view style="select-radio-outer">
      <view style="select-radio-inner"/>
    </view>
  </option>
  <view style="select-separator"/>
</select-multiple>

  


  
    <text style="description">Pre-selected</text>
  

<select-multiple name="pre-selected-select-multiple" style="select">
  <view style="select-separator"/>
  <option style="select-option" value="paid_parking">
    <text style="select-label">Paid parking</text>
    <view style="select-radio-outer">
      <view style="select-radio-inner"/>
    </view>
  </option>
  <view style="select-separator"/>
  <option selected="true" style="select-option" value="free_parking">
    <text style="select-label">Free parking</text>
    <view style="select-radio-outer">
      <view style="select-radio-inner"/>
    </view>
  </option>
  <view style="select-separator"/>
  <option selected="true" style="select-option" value="unknown_parking">
    <text style="select-label">Unknown</text>
    <view style="select-radio-outer">
      <view style="select-radio-inner"/>
    </view>
  </option>
  <view style="select-separator"/>
</select-multiple>

  


  
    <text style="description">Custom</text>
  

<select-multiple name="custom-select-multiple" style="select-custom">
  <option style="select-custom-option" value="paid_parking">
    <view style="select-custom-image-wrapper">
      <image
        source="/hyperview/public/ui/ui-elements/forms/select-multiple/unselected.png"
        style="select-custom-image select-custom-image-unselected"/>
      <image
        source="/hyperview/public/ui/ui-elements/forms/select-multiple/selected.png"
        style="select-custom-image select-custom-image-selected"/>
    </view>
    <text style="select-custom-label">Paid parking</text>
  </option>
  <option selected="true" style="select-custom-option" value="free_parking">
    <view style="select-custom-image-wrapper">
      <image
        source="/hyperview/public/ui/ui-elements/forms/select-multiple/unselected.png"
        style="select-custom-image select-custom-image-unselected"/>
      <image
        source="/hyperview/public/ui/ui-elements/forms/select-multiple/selected.png"
        style="select-custom-image select-custom-image-selected"/>
    </view>
    <text style="select-custom-label">Free parking</text>
  </option>
  <option style="select-custom-option" value="unknown_parking">
    <view style="select-custom-image-wrapper">
      <image
        source="/hyperview/public/ui/ui-elements/forms/select-multiple/unselected.png"
        style="select-custom-image select-custom-image-unselected"/>
      <image
        source="/hyperview/public/ui/ui-elements/forms/select-multiple/selected.png"
        style="select-custom-image select-custom-image-selected"/>
    </view>
    <text style="select-custom-label">Unknown</text>
  </option>
</select-multiple>


  
  
  
    <text style="button-serialize-title">To test form serialization, setup the Hyperview demo app on your local environment.</text>
    
  <view
    
      style="button button-disabled"
    
    
  >
    
    <text style="button-label">Serialize</text>
  </view>

  

</form>

