<doc xmlns="https://hyperview.org/hyperview"
     xmlns:ns1="https://hyperview.org/hyperview-intercom">&gt; 
<screen>
  <styles>
    <style alignItems="center"
           backgroundColor="white"
           flexDirection="row"
           height="72"
           id="Header"
           paddingLeft="24"
           paddingRight="24"
           paddingTop="24" />
    <style height="19"
           id="Header__Back"
           marginRight="16"
           width="24" />
    <style color="black"
           fontWeight="600"
           fontSize="24"
           id="Header__Title" />
    <style backgroundColor="white"
           flex="1"
           id="Body" />
    <style backgroundColor="white"
           flex="1"
           id="Main" />
    <style borderBottomColor="#E1E3E3"
           borderBottomWidth="1"
           id="Section"
           marginBottom="24"
           marginTop="24" />
    <style flexDirection="row"
           id="Section__Top"
           justifyContent="space-between" />
    <style fontWeight="bold"
           fontSize="20"
           id="Section__Header"
           lineHeight="24"
           marginBottom="16"
           marginLeft="24"
           marginRight="24" />
    <style color="#4778FF"
           fontWeight="600"
           fontSize="16"
           id="Section__Action"
           lineHeight="24"
           marginBottom="16"
           marginLeft="24"
           marginRight="24" />
    <style flex="1"
           flexDirection="row"
           id="ShiftItem"
           justifyContent="flex-start"
           marginBottom="32"
           paddingLeft="24"
           paddingRight="24" />
    <style borderRadius="24"
           height="48"
           id="ShiftItem__Avatar"
           width="48" />
    <style flex="1"
           flexDirection="column"
           id="ShiftItem__Info"
           marginLeft="16" />
    <style color="#4E4D4D"
           fontWeight="normal"
           fontSize="16"
           id="ShiftItem__WorkerName"
           lineHeight="24" />
    <style color="#8D9494"
           fontWeight="normal"
           fontSize="16"
           id="ShiftItem__Details"
           lineHeight="24" />
    <style flex="1"
           flexDirection="row"
           id="Rating"
           margin="0" />
    <style height="24"
           id="Rating__Star"
           marginRight="8"
           width="24" />
    <style backgroundColor="#FBB90E"
           id="Rating__Star--Filled" />
  </styles>
  <body style="Body">
    <header style="Header">
      <image action="back"
             href="/case_studies/index.xml"
             source="/case_studies/back@2x.png"
             style="Header__Back" />
    </header>
    <view scroll="true"
          style="Main">
      <view style="Section">
        <view style="Section__Top">
          <text style="Section__Header">Instaworkers</text>
          <text style="Section__Action">
          <behavior action="intercom"
                    trigger="press"
                    ns1:action="open" />Report Issue</text>
        </view>
        <list>
          <item action="new"
                delay="500"
                href="/case_studies/rating_modal.xml"
                key="a"
                show-during-load="loadingScreen"
                style="ShiftItem">
            <image source="/case_studies/image.jpg"
                   style="ShiftItem__Avatar" />
            <view style="ShiftItem__Info">
              <text style="ShiftItem__WorkerName">Jake Tepperman</text>
              <text style="ShiftItem__Details">7:00 PM - 1:00 AM - $116.80</text>
              <view style="Rating">
                <image action="new"
                       delay="500"
                       href="/case_studies/rating_modal.xml?rating=1"
                       show-during-load="loadingScreen"
                       source="/case_studies/star_empty.png"
                       style="Rating__Star" />
                <image action="new"
                       delay="500"
                       href="/case_studies/rating_modal.xml?rating=2"
                       show-during-load="loadingScreen"
                       source="/case_studies/star_empty.png"
                       style="Rating__Star" />
                <image action="new"
                       delay="500"
                       href="/case_studies/rating_modal.xml?rating=3"
                       show-during-load="loadingScreen"
                       source="/case_studies/star_empty.png"
                       style="Rating__Star" />
                <image action="new"
                       delay="500"
                       href="/case_studies/rating_modal.xml?rating=4"
                       show-during-load="loadingScreen"
                       source="/case_studies/star_empty.png"
                       style="Rating__Star" />
                <image action="new"
                       delay="500"
                       href="/case_studies/rating_modal.xml?rating=5"
                       show-during-load="loadingScreen"
                       source="/case_studies/star_empty.png"
                       style="Rating__Star" />
              </view>
            </view>
          </item>
          <item action="new"
                delay="500"
                href="/case_studies/rating_modal.xml"
                key="b"
                show-during-load="loadingScreen"
                style="ShiftItem">
            <image source="/case_studies/image.jpg"
                   style="ShiftItem__Avatar" />
            <view style="ShiftItem__Info">
              <text style="ShiftItem__WorkerName">Jake Tepperman</text>
              <text style="ShiftItem__Details">7:00 PM - 1:00 AM - $116.80</text>
              <view style="Rating">
                <image action="new"
                       delay="500"
                       href="/case_studies/rating_modal.xml?rating=1"
                       show-during-load="loadingScreen"
                       source="/case_studies/star_empty.png"
                       style="Rating__Star" />
                <image action="new"
                       delay="500"
                       href="/case_studies/rating_modal.xml?rating=2"
                       show-during-load="loadingScreen"
                       source="/case_studies/star_empty.png"
                       style="Rating__Star" />
                <image action="new"
                       delay="500"
                       href="/case_studies/rating_modal.xml?rating=3"
                       show-during-load="loadingScreen"
                       source="/case_studies/star_empty.png"
                       style="Rating__Star" />
                <image action="new"
                       delay="500"
                       href="/case_studies/rating_modal.xml?rating=4"
                       show-during-load="loadingScreen"
                       source="/case_studies/star_empty.png"
                       style="Rating__Star" />
                <image action="new"
                       delay="500"
                       href="/case_studies/rating_modal.xml?rating=5"
                       show-during-load="loadingScreen"
                       source="/case_studies/star_empty.png"
                       style="Rating__Star" />
              </view>
            </view>
          </item>
        </list>
      </view>
    </view>
  </body>
</screen>
<screen id="loadingScreen">
  <styles>
    <style alignItems="center"
           backgroundColor="white"
           borderBottomColor="#eee"
           borderBottomWidth="1"
           flexDirection="row"
           height="72"
           id="Header"
           paddingLeft="24"
           paddingRight="24"
           paddingTop="24" />
    <style height="32"
           id="Header__Close"
           marginRight="16"
           width="32" />
    <style backgroundColor="white"
           flex="1"
           id="Body" />
    <style color="black"
           fontWeight="bold"
           fontSize="32"
           id="Title"
           lineHeight="40"
           marginLeft="24"
           marginRight="24"
           marginTop="24" />
    <style alignItems="center"
           flex="1"
           id="Loading"
           justifyContent="center" />
  </styles>
  <body style="Body">
    <header style="Header">
      <image action="close"
             href="/index.xml"
             source="/case_studies/close@2x.png"
             style="Header__Close" />
    </header>
    <text style="Title">Rate the Instaworker</text>
    <view style="Loading">
      <spinner />
    </view>
  </body>
</screen></doc>
