---
title: "Tab Navigator - Tab 2"
permalink: "/navigator/custom_tabbar/tab_2.xml"
---

{% extends 'base.xml.njk' %}
{% block container %}
  <view scroll="true">
    <text style="Description">This is the content for tab 2.</text>
  </view>
{% endblock %}

{% block footer %}
  <footer>
    <view id="footer" style="Footer">
      <view
        style="Bottom_Tab"
        href-style="Bottom_Tab"
        action="navigate"
        target="tab-1"
      >
        <image
          source="/navigator/simple_tab/heart2.png"
          style="Bottom_Tab__Icon "
        />
        <text style="Bottom_Tab__Label">Tab 1</text>
      </view>
      <view
        style="Bottom_Tab"
        href-style="Bottom_Tab"
        action="navigate"
        target="tab-2"
      >
        <image
          source="/navigator/simple_tab/list2.png"
          style="Bottom_Tab__Icon "
        />
        <text style="Bottom_Tab__Label">Tab 2</text>
      </view>
    </view>
  </footer>
{% endblock %}
