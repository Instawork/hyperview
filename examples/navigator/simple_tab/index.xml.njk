      <navigator id="simple-tab-navigator" type="tab">
        <nav-route id="tab-1" href="/navigator/simple_tab/tab_1.xml"/>
        <nav-route id="tab-2" href="/navigator/simple_tab/tab_2.xml"/>
      </navigator>
    </nav-route>
  </navigator>
