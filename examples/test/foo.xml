<s3-file-upload:field
  xmlns:s3-file-upload="https://instawork.com/hyperview-s3-file-upload"
  id="interactive-unset"
  s3-file-upload:policy-url="http://localhost:8080/api/s3_direct_upload_params/"
  s3-file-upload:dest="user_avatar"
  s3-file-upload:choose-event="interactive-unset/choose"
  s3-file-upload:cancel-event="interactive-unset/cancel"
  s3-file-upload:retry-event="interactive-unset/retry"
  s3-file-upload:max-image-height="1080"
  s3-file-upload:max-image-width="1080"
  s3-file-upload:camera-type="front"
  s3-file-upload:camera-flash-type="auto"
  s3-file-upload:choose-camera-picker-event="interactive-unset/choose-camera"
  s3-file-upload:camera-cancel-event="interactive-unset/camera-cancel"
  s3-file-upload:camera-capture-event="interactive-unset/camera-capture"
  s3-file-upload:camera-type-front-event="interactive-unset/camera-type-front"
  s3-file-upload:camera-type-back-event="interactive-unset/camera-type-back"
  s3-file-upload:camera-flash-mode-on-event="interactive-unset/camera-flash-mode-on"
  s3-file-upload:camera-flash-mode-off-event="interactive-unset/camera-flash-mode-off"
  s3-file-upload:camera-flash-mode-auto-event="interactive-unset/camera-flash-mode-auto"
  s3-file-upload:choose-image-picker-event="interactive-unset/choose-image"
>
<text-field s3-file-upload:role="file-url-value" hide="true" name="field" />















<view
