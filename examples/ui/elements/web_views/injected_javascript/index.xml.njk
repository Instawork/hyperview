{% extends 'base.xml.njk' %}
{% block styles %}
  <style id="flex" flex="1" flexDirection="row" justifyContent="space-between"/>
  <style id="main" flex="1"/>
{% endblock %}
{% block container %}
  <view style="main">
    <view style="flex">
      <web-view url="https://hyperview.org/" activity-indicator-color="red" injected-java-script="alert('Hello world!')"/>
    </view>
  </view>
{% endblock %}
