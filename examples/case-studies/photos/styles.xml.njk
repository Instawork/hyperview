<style
  id="stories"
  backgroundColor="#F4F4F4"
  flex="1"
  flexDirection="row"
  paddingLeft="8"
/>
<style
  id="story"
  alignItems="center"
  flex="1"
  marginBottom="8"
  marginLeft="8"
  marginRight="8"
  marginTop="8"
/>
<style
  id="story-avatar"
  backgroundColor="#F4F4F4"
  borderRadius="32"
  height="64"
  width="64"
/>
<style
  id="story-username"
  fontSize="14"
  fontWeight="bold"
  marginTop="4"
/>
<style
  id="image-header"
  alignItems="center"
  backgroundColor="white"
  flex="1"
  flexDirection="row"
  justifyContent="space-between"
  paddingBottom="16"
  paddingLeft="16"
  paddingRight="16"
  paddingTop="16"
/>
<style
  id="image-header-avatar"
  backgroundColor="#F4F4F4"
  borderRadius="24"
  height="40"
  width="40"
/>
<style id="image-header-left" flexDirection="row" />
<style
  id="image-header-left-labels"
  justifyContent="center"
  marginLeft="8"
/>
<style
  id="image-header-username"
  color="#4E4D4D"
  fontSize="14"
  fontWeight="bold"
/>
<style
  id="image-header-location"
  color="#BDC4C4"
  fontSize="14"
  fontWeight="normal"
/>
<style id="image-header-right" flexDirection="row" />
<style
  id="ImageHeader__more"
  color="#BDC4C4"
  fontSize="14"
  fontWeight="bold"
/>
<style id="image-container" />
<style
  id="image-container-overlay"
  alignItems="center"
  flex="1"
  height="375"
  justifyContent="center"
  left="0"
  position="absolute"
  top="0"
  width="375"
/>
<style id="image" backgroundColor="#F4F4F4" height="375" width="375" />
<style
  id="image-message"
  backgroundColor="black"
  borderRadius="16"
  paddingBottom="8"
  paddingLeft="24"
  paddingRight="24"
  paddingTop="8"
/>
<style
  id="image-message-label"
  color="white"
  fontSize="14"
  fontWeight="bold"
/>
<style
  id="actionbar"
  flexDirection="row"
  justifyContent="space-between"
  marginLeft="16"
  marginRight="16"
  marginTop="8"
/>
<style id="actionbar-left" flexDirection="row" />
<style id="actionbar-right" flexDirection="row" />
<style
  id="actionbar-icon"
  height="24"
  marginLeft="4"
  marginRight="4"
  width="24"
/>
<style id="actionbar-icon-image" height="24" width="24">
  <modifier selected="true">
  <style height="0" width="0" />
  </modifier>
</style>
<style id="actionbar-icon-image-selected" height="0" width="0">
  <modifier selected="true">
  <style height="24" width="24" />
  </modifier>
</style>
<style id="actionbar-icon-first" marginLeft="0" />
<style id="actionbar-icon-last" marginRight="0" />
<style
  id="caption"
  color="#4E4D4D"
  fontSize="14"
  lineHeight="24"
  marginLeft="16"
  marginRight="16"
  marginTop="8"
/>
<style id="comments" marginLeft="16" marginRight="16" />
<style
  id="comments-likes"
  color="#4E4D4D"
  fontSize="14"
  fontWeight="bold"
  lineHeight="24"
/>
<style
  id="comments-more"
  color="#BDC4C4"
  fontSize="14"
  fontWeight="normal"
  lineHeight="24"
/>
<style id="comment" flexDirection="row" marginTop="4" />
<style
  id="comment-author"
  color="#4E4D4D"
  fontSize="14"
  fontWeight="bold"
  lineHeight="18"
  marginRight="4"
/>
<style
  id="comment-content"
  color="#4E4D4D"
  fontSize="14"
  fontWeight="normal"
  lineHeight="18"
/>
