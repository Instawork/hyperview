---
permalink: "/ui/index.xml"
hv_title: "UI"
hv_tab_id: "ui"
hv_button_behavior: "none"
---
{% extends 'templates/tab-route.xml.njk' %}

{% from 'macros/tabbar-top/index.xml.njk' import tabbar_top %}
{% from 'macros/tabbar-top-tab/index.xml.njk' import tabbar_top_tab %}

{% block tab_route_content %}
  {{ tabbar_top([
    {id: "tab-styling", label: "Styling", selected: true},
    {id: "tab-ui-elements", label: "UI Elements"}
  ]) }}
  {% call tabbar_top_tab("tab-styling", true) -%}
    {% include "./styling/content.xml.njk" %}
  {%- endcall -%}
  {% call tabbar_top_tab("tab-ui-elements") -%}
    {% include "./ui-elements/content.xml.njk" %}
  {%- endcall -%}
{% endblock %}
