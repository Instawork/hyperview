<list
  action="replace"
  href="/hyperview/public/ui/ui-elements/list/refresh/refresh_load.xml"
  style="list"
  trigger="refresh"
  xmlns="https://hyperview.org/hyperview"
>
  <item key="new_item" style="item">
    <text style="item-label">New Item</text>
  </item>
  <item key="1" style="item">
    <text style="item-label">List 1</text>
  </item>
  <item key="2" style="item">
    <text style="item-label">List 2</text>
  </item>
  <item key="another_new_item" style="item">
    <text style="item-label item-label-special">Another New Item</text>
  </item>
  <item key="3" style="item">
    <text style="item-label">List 3</text>
  </item>
  <item key="4" style="item">
    <text style="item-label">List 4</text>
  </item>
  <item key="5" style="item item-last">
    <text style="item-label">List 5</text>
  </item>
</list>
