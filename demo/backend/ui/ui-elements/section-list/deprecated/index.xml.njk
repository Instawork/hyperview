---
permalink: "/ui/ui-elements/section-list/deprecated/index.xml"
tags: "UI/UI Elements/Section List"
hv_title: "Deprecated Section List"
hv_button_behavior: "back"
---
{% extends 'templates/base.xml.njk' %}

{% block container %}
  <section-list id="myList">
    <section>
      <section-title style="list-header">
        <text style="list-header-text">Section 1</text>
      </section-title>
      {% for item in range(1, 6) %}
        <item key="{{ item }}" style="item">
          <text style="item-label">List {{ item }}</text>
        </item>
      {% endfor %}
      <item
        action="append"
        delay="1000"
        href="/hyperview/public/ui/ui-elements/section-list/deprecated/deprecated_scroll_page2.xml"
        key="20"
        once="true"
        show-during-load="myIndicator"
        style="item"
        target="myList"
        trigger="visible"
      >
        <text style="item-label">List 20</text>
      </item>
    </section>
  </section-list>
{% endblock %}
