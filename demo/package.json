{
  "name": "demo",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "deploy": "gh-pages -d hyperview",
    "format-njk": "./scripts/format-njk.sh",
    "predeploy": "BASE_URL=https://instawork.github.io expo export:web; rm -rf hyperview; mv -f web-build/ hyperview; ENV='production' eleventy",
    "prettify": "pretty-quick && prettier --write '**/*.md'",
    "server": "eleventy --watch --incremental --serve --port=8085",
    "test:lint": "eslint src && prettier --check '**/*.md' && yarn format-njk -c",
    "test:ts": "tsc",
    "test:validate-xml": "rm -rf hyperview/public; eleventy; find hyperview/public -name '*.xml' | xargs xmlschema-validate --schema schema/hyperview.xsd --version 1.1",
    "test": "yarn test:ts && yarn test:lint && yarn test:validate-xml"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "6.5.7",
    "@react-navigation/native": "6.1.6",
    "@react-navigation/stack": "6.3.16",
    "expo": "~53.0.17",
    "expo-constants": "~17.1.7",
    "expo-linking": "~7.1.7",
    "expo-status-bar": "~2.2.3",
    "hyperview": "0.101.0",
    "moment": "^2.30.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-server-browsersync": "^1.0.2",
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "7.13.4",
    "@babel/preset-env": "^7.28.0",
    "@expo/webpack-config": "^19.0.1",
    "@faker-js/faker": "^9.9.0",
    "@prettier/plugin-xml": "0.13.0",
    "@tsconfig/react-native": "^3.0.6",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/react": "~18.3.12",
    "@types/url-parse": "^1.4.11",
    "babel-eslint": "10.1.0",
    "eslint": "7.20.0",
    "eslint-plugin-instawork": "0.12.0",
    "gh-pages": "^6.3.0",
    "nunjucks": "^3.2.4",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "private": true,
  "homepage": ".",
  "engines": {
    "node": ">=20.0.0 <23.0.0"
  }
}
